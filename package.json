{
  "name": "asam-avx-osi-converter",
  "displayName": "ASAM AVX OSI Converter",
  "description": "Visualises data following the standard and Ansys-AVX of the ASAM Open Simulation Interface (ASAM OSI) using the native 3D panel of Lichtblick",
  "publisher": "Lichtblick",
  "homepage": "https://www.asam.net/standards/detail/osi/",
  "version": "0.0.1-avx",
  "license": "MIT",
  "main": "./dist/extension.js",
  "keywords": [
    "lichtblick",
    "asam",
    "osi",
    "visualization"
  ],
  "scripts": {
    "build": "foxglove-extension build",
    "foxglove:prepublish": "foxglove-extension build --mode production",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "local-install": "foxglove-extension install",
    "package": "foxglove-extension package",
    "test": "jest"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@foxglove/schemas": "^1.6.2",
    "@lichtblick/eslint-plugin": "^1.0.1",
    "@lichtblick/suite": "^1.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "asam-osi-types": "github:Lichtblick-Suite/asam-osi-types",
    "create-foxglove-extension": "0.8.6",
    "esbuild": "^0.20.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-essentials": "^9.4.1",
    "typescript": "5.3.3"
  }
